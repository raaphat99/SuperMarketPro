@model IEnumerable<Product>


<table id="products-table" class="table table-bordered">
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Available Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in @Model)
        {
            <tr class="product-info" data-product-id="@product.Id">
                <td>@product.Name</td>
                <td>@product.Quantity</td>
            </tr>
        }
    </tbody>
</table>




